<header class='site-header'>
  <div class="wrap wrap-head">
    <h1 class="site-logo">
      <a class="site-logo-link" href="<%-process.env.npm_package_reponame%>/">
        <span>ATX</span><span>Vegan</span>
      </a>
    </h1>
    <nav class="nav site-nav">
      <ul class="site-nav-list">
        <% for(var i = 0; i < nav.length; i++) {%>
          <% if(nav[i].Sub){ %>
            <li class="site-nav-list-item">
              <a class="site-nav-list-item-link toggle" href="/"><%=nav[i].Text%></a>
              <ul class="site-nav-sub-list">
                <% for(var j = 0; j < nav[i].Sub.length; j++) {%>
                  <li class="site-nav-sub-list-item"><a class="site-nav-sub-list-item-link" href="<%-process.env.npm_package_reponame%>/<%=nav[i].Sub[j].Link%>"><%=nav[i].Sub[j].Text%></a></li>
                <%}%>
              </ul>
            </li>
          <% } else{ %>
            <li class="site-nav-list-item"><a class="site-nav-list-item-link" href="<%-process.env.npm_package_reponame%>/<%=nav[i].Link%>"><%=nav[i].Text%></a></li>
          <% } %>
        <%}%>
      </ul>
    </nav>
    <div class="search">
      <input class="search-input" type="text" id="search" placeholder="search"/><button class="search-btn" id="search-btn"><svg fill="#000000" height="24" viewBox="0 0 24 24" width="24" xmlns="http://www.w3.org/2000/svg">
          <path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"/>
          <path d="M0 0h24v24H0z" fill="none"/>
      </svg></button>
    </div>
  </div>
</header>
